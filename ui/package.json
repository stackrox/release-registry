{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "npm-run-all clean:*",
    "clean:tailwind": "rm -f src/index.css",
    "start": "npm-run-all clean -p start:*",
    "start:tailwind": "NODE_ENV=development postcss ./src/app.tw.css -o ./src/index.css -w",
    "start:cra": "wait-on src/index.css && PORT=3001 HTTPS=true EXTEND_ESLINT=true react-scripts start",
    "build": "npm-run-all build:tailwind build:cra",
    "build:tailwind": "NODE_ENV=production postcss ./src/app.tw.css -o ./src/index.css",
    "build:cra": "EXTEND_ESLINT=true react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint-check": "npm-run-all lint-check:*",
    "lint-check:non-src": "prettier --check '**/*.{md,css,json}'",
    "lint-check:src": "eslint --ext .js,.jsx,.ts,.tsx ./",
    "lint": "npm-run-all lint:*",
    "lint:non-src": "prettier --write '**/*.{md,css,json}'",
    "lint:src": "yarn lint-check:src --fix",
    "gen:src": "./scripts/generate-client.sh"
  },
  "dependencies": {
    "@patternfly/patternfly": "^4.224.2",
    "@patternfly/react-core": "^4.276.8",
    "@stackrox/infra-auth-lib": "^0.0.4",
    "axios": "^0.21.4",
    "formik": "^2.4.4",
    "history": "^5.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "namor": "^3.0.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.9",
    "react-modal": "^3.14.4",
    "react-router-dom": "^6.3.0",
    "react-spinners": "^0.13.8",
    "use-clipboard-copy": "^0.2.0",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/helper-builder-react-jsx": "^7.19.0",
    "@babel/helper-builder-react-jsx-experimental": "^7.12.11",
    "@babel/plugin-syntax-flow": "^7.22.5",
    "@babel/plugin-transform-react-jsx": "^7.22.15",
    "@stackrox/tailwind-config": "^0.2.0",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.200",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.30",
    "@types/react-dom": "^18.2.14",
    "@types/react-modal": "^3.12.1",
    "autoprefixer": "^10.3.4",
    "eslint": "^8.50.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "http-proxy-middleware": "^2.0.1",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "prettier": "^3.0.3",
    "react-scripts": "^5.0.1",
    "tailwindcss": "^2.2.9",
    "typescript": "~4.3.5",
    "wait-on": "^7.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
